{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "target": "ES6",
    "paths": {
      "api/*": ["./api/*"],
      "assets/*": ["./assets/*"],
      "components/*": ["./components/*"],
      "contexts/*": ["./contexts/*"],
      "hooks/*": ["./hooks/*"],
      "screens/*": ["./screens/*"],
      "lib/*": ["./lib/*"],
      "setup/*": ["./setup/*"],
      "utils/*": ["./utils/*"]
    }
  },
  "include": [
    "./**/*",
    "./api/**/*",
    "./assets/**/*",
    "./components/**/*",
    "./contexts/**/*",
    "./hooks/**/*",
    "./screens/**/*",
    "./lib/**/*",
    "./setup/**/*",
    "./utils/**/*",
    "./consts/**/*",
    "nativewind-env.d.ts"
  ],
  "exclude": ["./android/**/*"]
}
